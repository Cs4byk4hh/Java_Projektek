package org.example;

public class Objects {
    String Fname;
    String Lname;
    int age;
    int SchoolClass;

    public Objects(String Fname, String Lname, int age, int Schoolclass) {
        this.Fname = Fname;
        this.Lname = Lname;
        this.age = age;
        this.SchoolClass = Schoolclass;
    }

    public String getFname() {
        return Fname;
    }
    public String getLname() {
        return Lname;
    }
    public int getAge() {
        return age;
    }
    public int getSchoolClass() {
        return SchoolClass;
    }

    @Override
    public String toString() {
        return ("Hi my First name is: " + this.getFname()
        + "\n my last name is: " + this.getLname() +
                "\n my age is: " + this.getAge() +
                "\n my class in school is: " + this.getSchoolClass());
    }

    public static void main(String[] args) {
        Objects student = new Objects("Alb√≥k", "Csaba", 17, 12);
        System.out.println(student.toString());
    }
}
